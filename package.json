{
  "name": "bootpack",
  "version": "0.13.0",
  "author": "ZachTRice",
  "description": "A webpack and bootstrap boilerplate template for building web pages quickly.",
  "license": "MIT",
  "homepage": "https://github.com/bootpack/bootpack#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bootpack/bootpack.git"
  },
  "bugs": {
    "url": "https://github.com/bootpack/bootpack/issues"
  },
  "keywords": [
    "webpack",
    "template",
    "webpage",
    "starter",
    "website"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "build:dev": "cross-env NODE_ENV=development webpack",
    "watch": "cross-env NODE_ENV=development webpack-dev-server --watch",
    "start": "cross-env NODE_ENV=development webpack-dev-server --open",
    "deploy:gh-pages": "node ./tools/deploy.js -- gh-pages",
    "test": "run-s lint:css lint:scss lint:js",
    "lint": "run-s lint:styles lint:js",
    "lint:css": "stylelint ./src/css/*.css",
    "lint:scss": "stylelint ./src/scss/*.scss",
    "lint:styles": "run-s lint:css lint:scss",
    "lint:js": "eslint .",
    "compress:images": "node ./tools/compress-images.js",
    "generate:images": "node ./tools/generate-images.js",
    "generate:images:multiple": "node ./tools/generate-images.js --multiple"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "jquery": "^3.4.1",
    "popper.js": "^1.15.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.6.1",
    "chalk": "^3.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "compress-images": "^1.5.2",
    "compression-webpack-plugin": "^3.0.1",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^7.0.1",
    "css-loader": "^3.0.0",
    "eslint": "^6.7.2",
    "eslint-config-import": "^0.13.0",
    "eslint-config-node": "^4.0.0",
    "eslint-config-promise": "^2.0.2",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.1",
    "exports-loader": "^0.7.0",
    "favicons-webpack-plugin": "1.0.2",
    "file-loader": "^4.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "js-image-generator": "^1.0.2",
    "mini-css-extract-plugin": "^0.11.0",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "sass-loader": "^7.3.1",
    "shelljs": "^0.8.3",
    "stylelint": "^11.1.1",
    "stylelint-config-twbs-bootstrap": "^0.9.0",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.8.2",
    "write-file-webpack-plugin": "^4.5.0",
    "yargs": "^15.1.0"
  }
}
